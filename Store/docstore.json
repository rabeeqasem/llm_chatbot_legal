u0627\u0629 \u0623\u0646 \u064a\u062d\u0644 \u0645\u062d\u0644 \u0627\u0644\u0639\u0636\u0648 \u0627\u0644\u0645\u062a\u0648\u0641\u064a \u0641\u064a \u0627\u0644\u062c\u0645\u0639\u064a\u0629 \u0623\u062d\u062f \u0648\u0631\u062b\u062a\u0647 \u0627\u0644\u0634\u0631\u0639\u064a\u064a\u0646 \u0623\u0648 \u0648\u0635\u064a\u0647\u0645 \u0645\u0645\u0646 \u062a\u0646\u0637\u0628\u0642 \u0639\u0644\u064a\u0647 \u0634\u0631\u0648\u0637 \u0627\u0644\u0639\u0636\u0648\u064a\u0629", "doc_id": "203b3ec5-5742-43af-8240-d05a52a3dc35", "embedding": null, "doc_hash": "744b36695da229e1aba971fd35d1a257de95e0f53680c86ef583094805af4823", "extra_info": null, "node_info": {"start": 4936, "end": 5990}, "relationships": {"1": "9c044530-d490-4a8a-8d04-d4287403501f", "2": "f42d6705-69a0-42b9-8d61-9fdeec420ef1", "3": "628a938f-e763-4a9e-97e1-8a37a9804e65"}}, "__type__": "1"}, "628a938f-e763-4a9e-97e1-8a37a9804e65": {"__data__": {"text": "\u0645\u0633\u0628\u0642\u0629 \u0645\u0646 \u0644\u0644\u062c\u0646\u0629 \u0627\u0644\u0625\u062f\u0627\u0631\u0629.\n\u0628\u200c-\t\u064a\u062c\u0648\u0632 \u0641\u064a \u062d\u0627\u0644 \u0632\u0648\u0627\u0644 \u0627\u0644\u0639\u0636\u0648\u064a\u0629 \u0628\u0633\u0628\u0628 \u0627\u0644\u0648\u0641\u0627\u0629 \u0623\u0646 \u064a\u062d\u0644 \u0645\u062d\u0644 \u0627\u0644\u0639\u0636\u0648 \u0627\u0644\u0645\u062a\u0648\u0641\u064a \u0641\u064a \u0627\u0644\u062c\u0645\u0639\u064a\u0629 \u0623\u062d\u062f \u0648\u0631\u062b\u062a\u0647 \u0627\u0644\u0634\u0631\u0639\u064a\u064a\u0646 \u0623\u0648 \u0648\u0635\u064a\u0647\u0645 \u0645\u0645\u0646 \u062a\u0646\u0637\u0628\u0642 \u0639\u0644\u064a\u0647 \u0634\u0631\u0648\u0637 \u0627\u0644\u0639\u0636\u0648\u064a\u0629 \u0628\u0639\u062f \u0627\u062e\u0630 \u0627\u0644\u0645\u0648\u0627\u0641\u0642\u0629 \u0645\u0646 \u0628\u0642\u064a\u0629 \u0627\u0644\u0648\u0631\u062b\u0629. \n\u062a\u200c-\t\u064a\u062c\u0648\u0632 \u0644\u0644\u0648\u0631\u062b\u0629 \u062a\u062d\u0648\u064a\u0644 \u0623\u0633\u0647\u0645 \u0645\u0648\u0631\u062b\u0647\u0645 \u0644\u0644\u063a\u064a\u0631 \u0634\u0631\u064a\u0637\u0629 \u0645\u0648\u0627\u0641\u0642\u0629 \u062c\u0645\u064a\u0639 \u0627\u0644\u0648\u0631\u062b\u0629\u060c \u0648\u0627\u0646\u0637\u0628\u0627\u0642 \u0634\u0631\u0648\u0637 \u0627\u0644\u0639\u0636\u0648\u064a\u0629 \u0639\u0644\u0649 \u0627\u0644\u0639\u0636\u0648 \u0627\u0644\u062c\u062f\u064a\u062f\u060c \u0648\u0627\u0644\u0645\u0648\u0627\u0641\u0642\u0629 \u0627\u0644\u0645\u0633\u0628\u0642\u0629 \u0645\u0646 \u0644\u0644\u062c\u0646\u0629 \u0627\u0644\u0625\u062f\u0627\u0631\u0629.\n\u062b\u200c-\t\u062a\u0646\u062a\u0642\u0644 \u062d\u0642\u0648\u0642 \u0648\u0627\u0644\u062a\u0632\u0627\u0645\u0627\u062a \u0627\u0644\u0639\u0636\u0648 \u0627\u0644\u0645\u062a\u0648\u0641\u0649 \u0625\u0644\u0649 \u0627\u0644\u0648\u0631\u062b\u0629\u060c \u0637\u0628\u0642\u0627\u064b \u0644\u0623\u062d\u0643\u0627\u0645 \u0627\u0644\u0645\u0648\u0627\u0631\u064a\u062b \u0627\u0644\u0645\u0639\u0645\u0648\u0644 \u0628\u0647\u0627 \u0628\u0639\u062f \u062d\u0633\u0645 \u0627\u0644\u062f\u064a\u0648\u0646 \u0627\u0644\u0645\u062a\u0631\u062a\u0628\u0629 \u0639\u0644\u0649 \u0627\u0644\u0639\u0636\u0648 \u0627\u0644\u0645\u062a\u0648\u0641\u0649 \u0644\u0644\u062c\u0645\u0639\u064a\u0629.\n\u062c\u200c-\t \u064a\u062c\u0648\u0632 \u0644\u0644\u0648\u0631\u062b\u0629 \u062a\u0648\u0643\u064a\u0644 \u0645\u0646 \u064a\u0646\u0648\u0628 \u0639\u0646\u0647\u0645 \u0641\u064a \u0625\u062f\u0627\u0631\u0629 \u062d\u0635\u0635 \u0645\u0648\u0631\u062b\u0647\u0645 \u0641\u0642\u0637\u060c \u0644\u062d\u064a\u0646 \u062a\u0635\u0648\u064a\u0628 \u0639\u0636\u0648\u064a\u0629 \u0645\u0648\u0631\u062b\u0647\u0645 \u0648\u0641\u0642\u0627\u064b \u0644\u0644\u0642\u0631\u0627\u0631 \u0628\u0642\u0627\u0646\u0648\u0646 \u0631\u0642\u0645 (20) \u0644\u0633\u0646\u0629 2017 \u0628\u0634\u0627\u0646 \u0627\u0644\u062c\u0645\u0639\u064a\u0627\u062a \u0627\u0644\u062a\u0639\u0627\u0648\u0646\u064a\u0629 \u0648\u0627\u0644\u0623\u0646\u0638\u0645\u0629 \u0627\u0644\u0635\u0627\u062f\u0631\u0629 \u0628\u0645\u0642\u062a\u0636\u0627\u0647\u060c \u062f\u0648\u0646 \u0623\u0646 \u064a\u062a\u0645\u062a\u0639 \u0627\u0644\u0634\u062e\u0635 \u0627\u0644\u0645\u0648\u0643\u0644 \u0628\u062d\u0642\u0648\u0642 \u0627\u0644\u0639\u0636\u0648\u064a\u0629.\n\n\u0627\u0644\u0645\u0627\u062f\u0629 (9) \u0627\u0644\u0625\u0646\u0627\u0628\u0629\n\u0644\u0644\u0639\u0636\u0648 \u0627\u0644\u0645\u062a\u0648\u0627\u062c\u062f \u062e\u0627\u0631\u062c \u0627\u0644\u0628\u0644\u0627\u062f \u0628\u0634\u0643\u0644 \u0645\u0624\u0642\u062a \u0627\u0648 \u062f\u0627\u062e\u0644 \u0627\u0644\u0628\u0644\u0627\u062f \u0623\u0646 \u064a\u0646\u064a\u0628 \u0639\u0646\u0647 \u062e\u0637\u064a\u0627\u064b \u0639\u0636\u0648\u0627\u064b \u0622\u062e\u0631 \u0644\u062d\u0636\u0648\u0631 \u0627\u062c\u062a\u0645\u0627\u0639\u0627\u062a \u0627\u0644\u0647\u064a\u0626\u0629 \u0627\u0644\u0639\u0645\u0648\u0645\u064a\u0629\u060c \u0648\u0627\u0644\u062a\u0635\u0648\u064a\u062a \u0639\u0644\u0649 \u0627\u0644\u0642\u0631\u0627\u0631\u0627\u062a\u060c \u0648\u0627\u0644\u0648\u0641\u0627\u0621 \u0628\u0627\u0644\u0627\u0644\u062a\u0632\u0627\u0645\u0627\u062a\u0647 \u0627\u0644\u0645\u0633\u062a\u062d\u0642\u0629 \u0639\u0644\u064a\u0647 \u0623\u062b\u0646\u0627\u0621 \u063a\u064a\u0627\u0628\u0647\u060c \u0648\u0644\u0627 \u064a\u062c\u0648\u0632 \u0644\u0644\u0639\u0636\u0648 \u0623\u0646 \u064a\u0646\u064a\u0628 \u0639\u0646\u0647 \u0623\u0643\u062b\u0631 \u0645\u0646 \u0639\u0636\u0648 \u0648\u0627\u062d\u062f\u060c \u0639\u0644\u0649 \u0623\u0646 \u064a\u062a\u0645 \u0625\u0642\u0631\u0627\u0631 \u0647\u0630\u0647 \u0627\u0644\u0625\u0646\u0627\u0628\u0629 \u0645\u0646 \u0644\u062c\u0646\u0629 \u0627\u0644\u0625\u062f\u0627\u0631\u0629.\n\n\u0627\u0644\u0645\u0627\u062f\u0629 (10) \u0632\u0648\u0627\u0644", "doc_id": "628a938f-e763-4a9e-97e1-8a37a9804e65", "embedding": null, "doc_hash": "d1611b1f135a0fca9dd88d61221dd5c9a639d9b4a45da6b324d6b23bb3b1c703", "extra_info": null, "node_info": {"start": 6001, "end": 6995}, "relationships": {"1": "9c044530-d490-4a8a-8d04-d4287403501f", "2": "203b3ec5-5742-43af-8240-d05a52a3dc35", "3": "b1649168-0581-44fa-a1a7-1533bde3ae61"}}, "__type__": "1"}, "b1649168-0581-44fa-a1a7-1533bde3ae61": {"__data__": {"text": "\u0628\u0627\u0644\u0627\u0644\u062a\u0632\u0627\u0645\u0627\u062a\u0647 \u0627\u0644\u0645\u0633\u062a\u062d\u0642\u0629 \u0639\u0644\u064a\u0647 \u0623\u062b\u0646\u0627\u0621 \u063a\u064a\u0627\u0628\u0647\u060c \u0648\u0644\u0627 \u064a\u062c\u0648\u0632 \u0644\u0644\u0639\u0636\u0648 \u0623\u0646 \u064a\u0646\u064a\u0628 \u0639\u0646\u0647 \u0623\u0643\u062b\u0631 \u0645\u0646 \u0639\u0636\u0648 \u0648\u0627\u062d\u062f\u060c \u0639\u0644\u0649 \u0623\u0646 \u064a\u062a\u0645 \u0625\u0642\u0631\u0627\u0631 \u0647\u0630\u0647 \u0627\u0644\u0625\u0646\u0627\u0628\u0629 \u0645\u0646 \u0644\u062c\u0646\u0629 \u0627\u0644\u0625\u062f\u0627\u0631\u0629.\n\n\u0627\u0644\u0645\u0627\u062f\u0629 (10) \u0632\u0648\u0627\u0644 \u0627\u0644\u0639\u0636\u0648\u064a\u0629: \u062a\u0632\u0648\u0644 \u0639\u0636\u0648\u064a\u0629 \u0627\u0644\u0639\u0636\u0648 \u0628\u0642\u0631\u0627\u0631 \u0645\u0633\u0628\u0628 \u0645\u0646 \u0644\u062c\u0646\u0629 \u0627\u0644\u0625\u062f\u0627\u0631\u0629\u060c \u0648\u062a\u0628\u0644\u064a\u063a \u0645\u0633\u0624\u0648\u0644 \u0647\u064a\u0626\u0629 \u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u062a\u0639\u0627\u0648\u0646\u064a \u0641\u064a \u0627\u0644\u0645\u062d\u0627\u0641\u0638\u0629 \u0641\u064a \u0627\u0644\u0623\u062d\u0648\u0627\u0644 \u0627\u0644\u062a\u0627\u0644\u064a\u0629:\n1.\t\u0641\u0642\u062f\u0627\u0646 \u0627\u0644\u0639\u0636\u0648 \u0623\u062d\u062f \u0645\u0624\u0647\u0644\u0627\u062a \u0627\u0644\u0639\u0636\u0648\u064a\u0629 \u0628\u0645\u0642\u062a\u0636\u0649 \u0647\u0630\u0627 \u0627\u0644\u0646\u0638\u0627\u0645\u060c \u0648\u0627\u0644\u0642\u0631\u0627\u0631 \u0628\u0642\u0627\u0646\u0648\u0646 \u0631\u0642\u0645 (20) \u0644\u0633\u0646\u0629 2017 \u0628\u0634\u0623\u0646 \u0627\u0644\u062c\u0645\u0639\u064a\u0627\u062a \u0627\u0644\u062a\u0639\u0627\u0648\u0646\u064a\u0629.\n2.\t\u0627\u0644\u0627\u0646\u0633\u062d\u0627\u0628 \u0628\u0639\u062f \u062a\u0628\u0644\u064a\u063a \u0623\u0645\u064a\u0646 \u0627\u0644\u0633\u0631 \u062e\u0637\u064a\u0627 \u0642\u0628\u0644 ...............\u060c \u0648\u0644\u0627 \u064a\u0639\u062a\u0628\u0631 \u0647\u0630\u0627 \u0627\u0644\u0627\u0646\u0633\u062d\u0627\u0628 \u0646\u0627\u0641\u0630\u0627 \u0637\u064a\u0644\u0629 \u0627\u0644\u0645\u062f\u0629 \u0627\u0644\u062a\u064a \u064a\u0643\u0648\u0646 \u0641\u064a\u0647\u0627 \u0627\u0644\u0639\u0636\u0648 \u0645\u062f\u064a\u0646\u0627 \u0644\u0644\u062c\u0645\u0639\u064a\u0629\u060c \u0623\u0648 \u0643\u0641\u064a\u0644\u0627 \u0639\u0644\u0649 \u0642\u0631\u0636 \u0644\u0645 \u064a\u0633\u062f\u062f \u0628\u0639\u062f \u0623\u0648 \u0644\u0645 \u064a\u0633\u062f\u062f \u0645\u0627 \u0639\u0644\u064a\u0647 \u0645\u0646 \u0627\u0644\u062a\u0632\u0627\u0645\u0627\u062a \u0623\u062e\u0631\u0649 \u0644\u0644\u062c\u0645\u0639\u064a\u0629.\n3.\t\u0627\u0644\u0641\u0635\u0644 \u0645\u0646 \u0639\u0636\u0648\u064a\u0629 \u0627\u0644\u062c\u0645\u0639\u064a\u0629 \u0648\u0641\u0642\u0627 \u0644\u0644\u0634\u0631\u0648\u0637 \u0648\u0627\u0644\u0625\u062c\u0631\u0627\u0621\u0627\u062a \u0627\u0644\u0645\u062d\u062f\u062f\u0629 \u0628\u0627\u0644\u0645\u0627\u062f\u0629 (10) \u0645\u0646 \u0647\u0630\u0627 \u0627\u0644\u0646\u0638\u0627\u0645.\n4.\t\u0627\u0644\u062c\u0646\u0648\u0646 \u0623\u0648 \u0627\u0644\u0648\u0641\u0627\u0629.\n5.\t\u0639\u0646\u062f \u0632\u0648\u0627\u0644 \u0627\u0644\u0639\u0636\u0648\u064a\u0629 \u0639\u0646 \u0627\u0644\u0639\u0636\u0648 \u0644\u0623\u064a \u0633\u0628\u0628 \u062a\u062f\u0641\u0639 \u0644\u0647 \u0623\u0633\u0647\u0645\u0647 \u062e\u0644\u0627\u0644...........\u0639\u0644\u0649 \u0623\u0646 \u064a\u062a\u0645 \u062a\u0633\u0648\u064a\u0629 \u062d\u0642\u0648\u0642\u0647 \u0648\u0627\u0644\u062a\u0632\u0627\u0645\u0627\u062a\u0647 \u0641\u064a \u0646\u0647\u0627\u064a\u0629 \u0627\u0644\u0633\u0646\u0629 \u0627\u0644\u0645\u0627\u0644\u064a\u0629 \u0644\u0644\u062c\u0645\u0639\u064a\u0629 \u0627\u0644\u062a\u064a \u0632\u0627\u0644\u062a \u0639\u0636\u0648\u064a\u062a\u0647 \u0641\u064a\u0627. \n\n\u0627\u0644\u0645\u0627\u062f\u0629 (11): \u0641\u0635\u0644 \u0627\u0644\u0623\u0639\u0636\u0627\u0621: \u064a\u0641\u0635\u0644 \u0627\u0644\u0639\u0636\u0648 \u0645\u0646 \u0627\u0644\u062c\u0645\u0639\u064a\u0629 \u0628\u0642\u0631\u0627\u0631 \u064a\u062a\u062e\u0630\u0647 ................... \u0623\u0639\u0636\u0627\u0621 \u0644\u062c\u0646\u0629 \u0627\u0644\u0625\u062f\u0627\u0631\u0629 \u0644\u0623\u062d\u062f \u0627\u0644\u0623\u0633\u0628\u0627\u0628 \u0627\u0644\u062a\u0627\u0644\u064a\u0629:\n1.\t\u0625\u0630\u0627 \u062d\u0643\u0645 \u0639\u0644\u064a\u0647 \u0628\u062c\u0646\u0627\u064a\u0629 \u0623\u0648 \u062c\u0646\u062d\u0629 \u0645\u062e\u0644\u0629 \u0628\u0627\u0644\u0634\u0631\u0641 \u0648\u0627\u0644\u0623\u0645\u0627\u0646\u0629 \u0627\u0644\u0639\u0627\u0645\u0629 \u0648\u0641\u0642\u0627\u064b \u0644\u0644\u0642\u0627\u0646\u0648\u0646.\n2.\t\u0625\u0630\u0627 \u0644\u0645 \u064a\u0624\u062f \u0645\u0627 \u0639\u0644\u064a\u0647 \u0645\u0646", "doc_id": "b1649168-0581-44fa-a1a7-1533bde3ae61", "embedding": null, "doc_hash": "5deb6b197566e1c9639d97362368a72c52eaa06bce5a591db62a58d0b057b9e9", "extra_info": null, "node_info": {"start": 7003, "end": 8071}, "relationships": {"1": "9c044530-d490-4a8a-8d04-d4287403501f", "2": "628a938f-e763-4a9e-97e1-8a37a9804e65", "3": "bb077762-e687-4bbc-b367-39f9aae506ea"}}, "__type__": "1"}, "bb077762-e687-4bbc-b367-39f9aae506ea": {"__data__": {"text": "\u0644\u062c\u0646\u0629 \u0627\u0644\u0625\u062f\u0627\u0631\u0629 \u0644\u0623\u062d\u062f \u0627\u0644\u0623\u0633\u0628\u0627\u0628 \u0627\u0644\u062a\u0627\u0644\u064a\u0629:\n1.\t\u0625\u0630\u0627 \u062d\u0643\u0645 \u0639\u0644\u064a\u0647 \u0628\u062c\u0646\u0627\u064a\u0629 \u0623\u0648 \u062c\u0646\u062d\u0629 \u0645\u062e\u0644\u0629 \u0628\u0627\u0644\u0634\u0631\u0641 \u0648\u0627\u0644\u0623\u0645\u0627\u0646\u0629 \u0627\u0644\u0639\u0627\u0645\u0629 \u0648\u0641\u0642\u0627\u064b \u0644\u0644\u0642\u0627\u0646\u0648\u0646.\n2.\t\u0625\u0630\u0627 \u0644\u0645 \u064a\u0624\u062f \u0645\u0627 \u0639\u0644\u064a\u0647 \u0645\u0646 \u0627\u0644\u062a\u0632\u0627\u0645\u0627\u062a \u0644\u0644\u062c\u0645\u0639\u064a\u0629 \u0623\u0648 \u0623\u0647\u0645\u0644 \u0623\u0648 \u0631\u0641\u0636 \u0627\u0644\u062a\u0642\u064a\u062f \u0628\u0647\u0630\u0627 \u0627\u0644\u0646\u0638\u0627\u0645 \u0623\u0648 \u0623\u064a \u0646\u0638\u0627\u0645 \u0639\u0627\u0645 \u0623\u0648 \u0642\u0627\u0646\u0648\u0646 \u0627\u0644\u062c\u0645\u0639\u064a\u0627\u062a \u0627\u0644\u062a\u0639\u0627\u0648\u0646\u064a\u0629 \u0623\u0648 \u0639\u062f\u0645 \u0627\u0644\u0631\u0636\u0648\u062e \u0644\u0642\u0631\u0627\u0631\u0627\u062a \u0627\u0644\u0647\u064a\u0626\u0629 \u0627\u0644\u0639\u0645\u0648\u0645\u064a\u0629 \u0648\u0644\u062c\u0646\u0629 \u0627\u0644\u0625\u062f\u0627\u0631\u0629 \u0628\u0639\u062f \u0625\u0639\u0637\u0627\u0626\u0647 \u0625\u0634\u0639\u0627\u0631\u0627 \u062e\u0637\u064a\u0627 \u0628\u0630\u0644\u0643.\n3.\t\u0625\u0630\u0627 \u062e\u0627\u0644\u0641 \u0646\u0635 \u0633\u0646\u062f \u0627\u0644\u062a\u0639\u0647\u062f \u0627\u0644\u062e\u0637\u064a \u0627\u0644\u0645\u062d\u062f\u062f \u0628\u0635\u064a\u063a\u0629 \u0637\u0644\u0628 \u0627\u0644\u0627\u0646\u062a\u0633\u0627\u0628 \u0644\u0644\u062c\u0645\u0639\u064a\u0629 \u0623\u0648 \u0639\u0642\u062f \u0627\u0644\u062a\u0623\u0633\u064a\u0633 \u0623\u0648 \u0627\u0644\u0639\u0642\u062f \u0623\u0648 \u0627\u062a\u0641\u0627\u0642 \u0627\u0644\u0627\u0644\u062a\u0632\u0627\u0645 \u0627\u0644\u0645\u0646\u0638\u0645 \u0623\u0648 \u0627\u0644\u0645\u0628\u0631\u0645 \u0623\u0648 \u0627\u0644\u0645\u0639\u0642\u0648\u062f \u0628\u064a\u0646\u0647 \u0648\u0628\u064a\u0646 \u0627\u0644\u062c\u0645\u0639\u064a\u0629 \u0648\u064a\u0634\u062a\u0631\u0637 \u0641\u064a \u0630\u0644\u0643 \u0623\u0646 \u062a\u0643\u0648\u0646 \u0644\u062c\u0646\u0629 \u0627\u0644\u0625\u062f\u0627\u0631\u0629 \u0642\u062f \u0623\u062e\u0637\u0631\u062a\u0647 \u0623\u0648 \u0623\u0646\u0630\u0631\u062a\u0647 \u0628\u0627\u0644\u0645\u062e\u0627\u0644\u0641\u0629 \u062e\u0637\u064a\u0627\u064b \u0648\u0644\u0645 \u064a\u0639\u0645\u0644 \u0639\u0644\u0649 \u062a\u0644\u0627\u0641\u064a\u0647\u0627 \u062e\u0644\u0627\u0644 \u0627\u0644\u0645\u062f\u0629 \u0627\u0644\u0645\u062d\u062f\u062f\u0629 \u0641\u064a \u0627\u0644\u0625\u062e\u0637\u0627\u0631\u060c \u0645\u0627 \u0644\u0645 \u064a\u0642\u062f\u0645 \u0639\u0630\u0631\u0627 \u062a\u0642\u062a\u0646\u0639 \u0628\u0647 \u0644\u062c\u0646\u0629 \u0627\u0644\u0625\u062f\u0627\u0631\u0629 \u062e\u0644\u0627\u0644 \u0641\u062a\u0631\u0629 \u0627\u0644\u0625\u062e\u0637\u0627\u0631.\n4.\t.............................................................................................\n5.\t.............................................................................................\n6.\t\u064a\u0628\u0644\u063a \u0627\u0644\u0639\u0636\u0648 \u0642\u0631\u0627\u0631 \u0641\u0635\u0644\u0647 \u0645\u0646 \u0627\u0644\u062c\u0645\u0639\u064a\u0629 \u0628\u0643\u062a\u0627\u0628 \u0645\u0633\u062c\u0644 (\u0645\u0636\u0645\u0648\u0646) \u062a\u0631\u0633\u0644\u0647 \u0625\u0644\u064a\u0647 \u0644\u062c\u0646\u0629 \u0627\u0644\u0625\u062f\u0627\u0631\u0629\u060c \u0648\u062a\u0628\u0644\u063a\u0647 \u0628\u0647 \u062e\u0637\u064a\u0627\u064b \u0628\u0623\u064a\u0629 \u0648\u0633\u064a\u0644\u0629 \u0642\u0627\u0646\u0648\u0646\u064a\u0629 \u062a\u0631\u0647\u0627 \u0645\u0646\u0627\u0633\u0628\u0629.  \n\n\u0627\u0644\u0645\u0627\u062f\u0629 (12): \u0627\u0633